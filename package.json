{
  "name": "download-artifact",
  "version": "2.0.0",
  "description": "Download a build artifact that was previously uploaded in the workflow by the upload-artifact action",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "release": "ncc build src/download-artifact.ts && git add -f dist/",
    "check-all": "concurrently \"npm:format-check\" \"npm:lint\" \"npm:build\"",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/download-artifact.git"
  },
  "keywords": [
    "Actions",
    "GitHub",
    "Artifacts",
    "Download"
  ],
  "author": "GitHub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/actions/download-artifact/issues"
  },
  "homepage": "https://github.com/actions/download-artifact#readme",
  "dependencies": {
    "@actions/artifact": "^0.5.2",
    "@actions/core": "^1.2.6"
  },
  "devDependencies": {
    "@types/node": "^12.12.6",
    "@typescript-eslint/parser": "^2.30.0",
    "@zeit/ncc": "^0.22.1",
    "concurrently": "^5.2.0",
    "eslint": "^7.4.0",
    "eslint-plugin-github": "^4.1.1",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  }
}
